{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 13
    },
    "rules": {
        "linebreak-style": [
            "error",
            "unix"
        ],
        "quotes": [
            "error",
            "double"
        ],
        "no-console":"off",
        "semi": "off",
        "eol-last": "off",
        "no-multiple-empty-lines":"off",
        "indent":["error",4],
        "max-len":"off",
        "comma-dangle":"off",
        "no-trailing-spaces":"off",
        "no-async-promise-executor":"off",
        "default-param-last":"off"
    }
}
